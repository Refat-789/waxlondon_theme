            "text_content": "<p>Longevity<\/p>",
            "font_size": 48,
            "font_size_mobile": 36,
            "text_line_height": 54,
            "text_line_height_mobile": 42,
            "text_color": "#333333",
            "font_family": "ITCClearface",
            "font_weight": "bold",
