        "single_text_block_YGtAHb": {
          "type": "single-text-block",
          "settings": {
            "text_content": "<p>International</p>",
            "font_size": 16,
            "font_size_mobile": 16,
            "text_line_height": 25,
            "text_line_height_mobile": 25,
            "text_color": "#333333",
            "font_family": "Maison Neue",
            "font_weight": "bold",
