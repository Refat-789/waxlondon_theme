        "single_text_block_tLVQgH": {
          "type": "single-text-block",
          "settings": {
            "text_content": "<ul><li><a href=\"https://omniparcelreturns.com/bookreturn/booking_return/313\" title=\"https://omniparcelreturns.com/bookreturn/booking_return/313\"><span style=\"text-decoration:underline\"><strong>UK Returns Portal</strong></span></a></li><li><a href=\"https://web.global-e.com/Returns/Portal/myn38\" title=\"https://web.global-e.com/Returns/Portal/myn38\"><span style=\"text-decoration:underline\"><strong>International Returns Portal</strong></span></a></li></ul>",
