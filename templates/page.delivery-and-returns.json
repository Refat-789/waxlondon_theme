        },
        "single_text_block_V9ELK3": {
          "type": "single-text-block",
          "settings": {
            "text_content": "<p>An International return is estimated to be around £20/USD 30 to return. You will get a quote for the cost of the return, depending on the distance your parcel has to travel.</p><p>If you are looking to receive a refund, this will be deducted from the refund.</p><p>If you are looking to exchange your items, please let our customer service team know at <span style=\"text-decoration:underline\">customercare@waxlondon.com</span>.</p>",
            "font_size": 16,
            "font_size_mobile": 16,
            "text_line_height": 25,
            "text_line_height_mobile": 25,
            "text_color": "#333333",
            "font_family": "Maison Neue",
            "font_weight": "normal",
            "text_alignment": "left",
            "text_alignment_mobile": "left",
            "block_width": "small"
